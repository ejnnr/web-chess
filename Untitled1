var status= '';
			var moveColor = 'White';
  				if (game.turn() === 'b') {
    			moveColor = 'Black';
  			}
  			status = moveColor + ' to move';
		    if (game.in_check() === true) {
		      status += ', ' + moveColor + ' is in check';
		    }
				statusEl.html(status);
				fenEl.html(game.fen());
				pgnEl.html(game.pgn());
			};